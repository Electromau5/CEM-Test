  <div class = "container-fluid">
    <div class = "row">
    	<% obj.each do |post| %>
		  <div class = "col-xs-6 col-md-3">
		    <div class = "thumbnail">
            <%= link_to image_tag(post.image.url(:medium)), post %>
             <div class = "post_index_title">
			  <p><%= post.title %></p>
		     </div>
		    </div>
          </div>
        <% end %>
	</div>
  </div>
