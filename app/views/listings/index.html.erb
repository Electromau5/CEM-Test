  <div class = "container-fluid">
    <div class = "row">
		<% @listing.each do |listing| %>
		  <div class = "col-xs-6 col-md-3">
		    <div class = "thumbnail">
		    <p><%= listing.name %></p>
            <%= link_to image_tag(listing.image.url(:medium)), listing %>
		    </div>
          </div>
		<% end %>
	</div>
  </div>	  
		