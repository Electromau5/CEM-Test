<div class = "container-fluid">
	<div class="btn-group btn-group-justified" role="group">
    	<% @list.each do |category| %>
			<div class="btn-group" role="group">
    			<button type="button">
    				<%= link_to "#{category.name}", category_path(category), class: "btn btn-default" %>
				</button>
			</div>
         <% end %>
	</div>
</div>
<% if logged_in? && current_seller.admin %>
	<%= link_to 'Delete Category', category_path(@category), method: :delete %>
<% end %>
<%= render 'posts/post', obj: @category_posts %>